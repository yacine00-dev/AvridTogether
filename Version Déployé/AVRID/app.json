{
  "expo": {
    "name": "AvridTogether",
    "slug": "avrid-together",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avridtogether.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.avridtogether.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/AppIcon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5c638e8e-14bd-4373-b9c9-b4c1686fa74a"
      }
    }
  }
}
